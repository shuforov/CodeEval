#Swap Case https://www.codeeval.com/open_challenges/96/submit/

def swap_case(String_o):
	end_str = []
	for a in String_o:
		if a == str.lower(a):
			k = str.upper(a)
			end_str.append(k)
		elif a == str.upper(a):
			l = str.lower(a)
			end_str.append(l)
	return "".join(end_str)

print swap_case("Hello world!")
print swap_case("JavaScript language 1.8")
print swap_case("A letter")